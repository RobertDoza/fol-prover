%{
#pragma GCC diagnostic ignored "-Wunused-function"
#include <iostream>
#include "term.hpp"
#include "atom.hpp"
#include "parser.tab.hpp"
%}

%option noyywrap

%%

True { return TRUE; }

False { return FALSE; }

[A-Z][a-zA-Z0-9_]* { yylval.str = strdup(yytext); return VAR; }

[a-z][a-zA-Z0-9_]* { yylval.str = strdup(yytext); return SYMBOL; }

~ { return NOT; }

& { return AND; }

\| { return OR; }

=> { return IMPLIES; }

\<=\> { return IFF; }

! { return FORALL; }

\? { return EXISTS; }

\. { return *yytext; }

[ \t\n]+ { }

. { return *yytext; }

%%

